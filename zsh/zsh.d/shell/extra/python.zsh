# -*- mode: shell-script[zsh] -*
#
# Summary: Adds Python-specific aliases to ZSH.

alias deenv='source deactivate'
alias mkenv='/usr/bin/python3 -m venv'
alias no2='/usr/bin/python3 -m nose2'
alias pp3='/usr/bin/pip3'
alias pp3g='/usr/bin/pip freeze | grep -i'
alias pp3i='/usr/bin/pip3 install --user'
alias pp3r='/usr/bin/pip3 remove'
alias py3='/usr/bin/python3'
alias py3srv='/usr/bin/python3 -m http.server'
